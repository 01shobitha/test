# type: Content of: <chapter><sect1><itemizedlist><listitem><itemizedlist><listitem><para>
#: en/dvd.xml:58
#, no-wrap
msgid ""
"\nthe IP address of the machine you want to unicast\n"
"to;\n"
"\n"
msgstr ""

# type: Content of: <chapter><sect1><sect2><sect3><itemizedlist><listitem><itemizedlist><listitem><para>
#: en/dvd.xml:64 en/encoding-cards.xml:365
#, no-wrap
msgid ""
"\nor the DNS name the machine you want to unicast\n"
"to;\n"
"\n"
