# type: Content of: <chapter><title>
#: en/examples.xml:2
#, no-wrap
msgid ""
"\nExamples for advanced use of VLC's stream output\n"
"(transcoding, multiple streaming, etc...)\n"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:7
#, no-wrap
msgid "\nTranscoding\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:11
#, no-wrap
msgid ""
"\nTranscode the input stream and send it to a\n"
"multicast IP address with the associated SAP announce:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:16
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128,deinterlace}:\n"
"standard{access=udp,mux=ts,url=239.255.12.42,sap,name=\"TestStream\"}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:21
#, no-wrap
msgid ""
"\n Display the input stream, transcode it and send it\n"
"to a multicast IP address with the associated SAP announce:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:26
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#duplicate{dst=display,dst=\n"
"\"transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128,deinterlace}:\n"
"standard{access=udp,mux=ts,url=239.255.12.42,sap,name=\"TestStream\"}\"}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:32
#, no-wrap
msgid ""
"\nTranscode the input stream, display the transcoded\n"
"stream and send it to a multicast IP address with the associated SAP\n"
"announce:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:38
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128,deinterlace}:\n"
"duplicate{dst=display,dst=standard{access=udp,mux=ts,url=239.255.12.42,sap,name=\"TestStream\"}}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:45
#, no-wrap
msgid "\nMultiple streaming\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:49
#, no-wrap
msgid ""
"\nSend a stream to a multicast IP address and a\n"
"unicast IP address:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:54
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#duplicate{dst=\n"
"standard{access=udp,mux=ts,url=239.255.12.42,sap,name=\"TestStream\"},\n"
"dst=standard{access=udp,mux=ts,url=192.168.1.2}}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:60
#, no-wrap
msgid ""
"\nDisplay the stream and send it to two unicast IP\n"
"addresses:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:65
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#duplicate{dst=display,dst=\n"
"standard{access=udp,mux=ts,url=192.168.1.12},\n"
"dst=standard{access=udp,mux=ts,url=192.168.1.42}}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:71
#, no-wrap
msgid "Send parts of a multiple program input stream:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:74
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv multiple_program_input_stream \n"
"--sout "
"'#duplicate{dst=standard{access=udp,mux=ts,url=239.255.12.42},select=\"program=12345\",\n"
"dst=standard{access=udp,mux=ts,url=239.255.12.43}, "
"select=\"video,program=1234-2345\"}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:80
#, no-wrap
msgid ""
"This command sends the program of the input stream which id is\n"
"12345 to 239.255.12.42 and all video programs with id between 1234 and\n"
"2345 to 239.255.12.43."
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:86
#, no-wrap
msgid ""
"\n\n"
"Transcoding and multiple streaming\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:91
#, no-wrap
msgid ""
"\nTranscode the input stream, display the transcoded\n"
"stream and send it to a multicast IP address with the associated SAP\n"
"announce and an unicast IP address:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:97
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128,deinterlace}:\n"
"duplicate{dst=display,dst=standard{access=udp,mux=ts,url=239.255.12.42,sap,name=\"TestStream\"},\n"
"dst=standard{access=udp,mux=ts,url=192.168.1.2}}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:103
#, no-wrap
msgid ""
"\nDisplay the input stream, transcode it and send it\n"
"to two unicast IP addresses:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:108
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#duplicate{dst=display,\n"
"dst=\"transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128}:\n"
"duplicate{dst=standard{access=udp,mux=ts,url=192.168.1.2},\n"
"dst=standard{access=udp,mux=ts,url=192.168.1.12}\"}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:115
#, no-wrap
msgid ""
"\nSend the input stream to a multicast IP address\n"
"and the transcoded stream to another multicast IP address with the\n"
"associated SAP announces:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:121
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#duplicate{dst=\n"
"standard{access=udp,mux=ts,url=239.255.1.2,sap,name=\"OriginalStream\"},\n"
"dst=\"transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128}:\n"
"standard{access=udp,mux=ts,url=239.255.1.3,sap,name=\"TranscodedStream\"}\"}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:130
#, no-wrap
msgid "\nHTTP streaming\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:134
#, no-wrap
msgid "\nStream in HTTP:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/examples.xml:140 en/examples.xml:193
#, no-wrap
msgid "\non the server, run:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:144
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#standard{access=http,mux=ogg,url=server.example.org:8080}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/examples.xml:150 en/examples.xml:203
#, no-wrap
msgid "\non the client(s), run:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:154
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc "
"http://server.example.org:8080</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:162
#, no-wrap
msgid "\nTranscode and stream in HTTP:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:166
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#transcode{vcodec=mp4v,acodec=mpga,vb=800,ab=128}:\n"
"standard{access=http,mux=ogg,url=server.example.org:8080}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:171
#, no-wrap
msgid ""
"\nFor example, if you want to stream an audio CD in\n"
"Ogg/Vorbis over HTTP:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:176
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv cdda:/dev/cdrom --sout "
"'#transcode{acodec=vorb,ab=128}:\n"
"standard{access=http,mux=ogg,url=server.example.org:8080}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:183
#, no-wrap
msgid "\nRTP streaming\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:187
#, no-wrap
msgid "\nStream in RTP:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:197
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#rtp{dst=192.168.0.12,port=1234,sdp=http://server.example.org:8080/test.sdp}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:207
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc "
"http://server.example.org:8080/test.sdp</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:217
#, no-wrap
msgid "\nUse the <emphasis>es</emphasis> module\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:221
#, no-wrap
msgid "\nSeparate audio and video in two PS files:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:225
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout\n"
"'#es{access=file,mux=ps,url_audio=audio-%c.%m,url_video=video-%c.%m}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:230
#, no-wrap
msgid ""
"\nExtract the audio track of the input stream to a TS\n"
"file:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:235
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#es{access_audio=file,mux_audio=ts,url_audio=audio-%c.%m}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:239
#, no-wrap
msgid ""
"\n Stream in unicast the audio track on a port and the\n"
"video track on another port:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/examples.xml:246
#, no-wrap
msgid "\non the server side:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:250
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout\n"
"'#es{access=rtp,mux=ts,url_audio=192.168.1.2:1212,url_video=192.168.1.2:1213}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/examples.xml:257
#, no-wrap
msgid "\non the client side: to receive the audio:\n"
