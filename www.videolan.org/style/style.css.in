/* =================================== THEME RECOLORING RULES ====================================== */

/* ORANGE */

.orange a, .orange h1, .orange #nav a, .orange #nav .link:hover ul li a, .orange h2
{
    color: #F48B00;
}

.orange .donate2 button:hover,
.orange .donate2 a:hover,
.orange #nav ul li > a:hover {
    background-color: #F09D30 !important; /* rgba(244, 139, 0, 0.8) */
    color: white;
}

.orange #donate .button,
.orange .donate2 button,
.orange #nav .donate2 a,
.orange #nav ul li.open > a,
.orange .btn-group.open .dropdown-toggle {
    background-color: #F48B00 !important;
    color: white;
}

.orange #nav ul li.active > a {
    background-color: #F8EEE0; /* rgba(244, 139, 0, 0.1); */
}

.orange #donate .button,
.orange .donate2 button,
.orange .open>.dropdown-toggle.btn-default:hover,
.orange .open>.dropdown-toggle.btn-default:focus,
.orange .btn-default:active:hover,
.orange .open>.dropdown-toggle.btn-default:active,
.orange .open>.dropdown-toggle.btn-default,
.orange .btn-default {
    border: 1px solid #E47B00;
    color: white;
}

.orange .btn-default {
    color: white;
    /* gradient disable fallback */
    background-color: #F48B00;
    background-image: -webkit-linear-gradient(top, #F48B00 0%, #F09D30 100%);
    background-image: -o-linear-gradient(top, #F48B00 0%, #F09D30 100%);
    background-image: -webkit-gradient(linear, left top, left bottom, from(#F48B00), to(#F09D30));
    background-image: linear-gradient(to bottom, #F48B00 0%, #F09D30 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F48B00', endColorstr='#F09D30', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
    background-repeat: repeat-x;
}

.orange .btn-default:hover,
.orange .btn-default:focus {
    background-color: #F09D30;
    background-position: 0 -20px;
}

.orange #downloadButton, .orange .normalButton
{
    color: white;
    background: #F48B00 url(//images.videolan.org/style/images/downloadButtonGradientOrange.png) repeat-x;
}

.orange #downloadButton:hover, .orange .normalButton:hover
{
    background: #F48B00 url(//images.videolan.org/style/images/downloadButtonGradientOrangeBright.png) repeat-x;
}

.orange .screenshot
{
    border: 1px solid #F48B00;
}

