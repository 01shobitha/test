# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:261
#, no-wrap
msgid "\n<prompt>% </prompt><userinput>vlc udp:@:1212</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/examples.xml:263
#, no-wrap
msgid "\nto receive the video:\n"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><screen>
#: en/examples.xml:267
#, no-wrap
msgid "\n<prompt>% </prompt><userinput>vlc udp:@:1213</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><para>
#: en/examples.xml:275
#, no-wrap
msgid ""
"\nStream in multicast the video and dump the audio in\n"
"a file:\n"
msgstr ""

# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:280
#, no-wrap
msgid ""
"\n<prompt>% </prompt><userinput>vlc -vvv input_stream --sout "
"'#es{access_video=udp,mux_video=ts,url_video=239.255.12.42,\n"
"access_audio=file,mux_audio=ps,url_audio=audio-%c.%m}'</userinput>\n"
msgstr ""

# type: Content of: <chapter><sect1><note><para>
#: en/examples.xml:285
