"<prompt>% </prompt><userinput>vlc -vvv input_stream --sout '#es{access_video=udp,mux_video=ts,url_video=239.255.12.42,\n"
"access_audio=file,mux_audio=ps,url_audio=audio-%c.%m}'</userinput>"
