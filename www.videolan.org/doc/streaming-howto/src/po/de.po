# type: Content of: <chapter><sect1><screen>
#: en/examples.xml:177
#, no-wrap
msgid ""
"<prompt>% </prompt><userinput>vlc -vvv cdda:/dev/cdrom --sout '#transcode{acodec=vorb,ab=128}:\n"
"standard{access=http,mux=ogg,dst=server.example.org:8080}'</userinput>"
msgstr ""

# type: Content of: <chapter><sect1><title>
#: en/examples.xml:184
#, no-wrap
