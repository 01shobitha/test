msgstr "Filtre d'inversion de la vidéo"

# type: Content of: <chapter><sect1><sect2><sect3><para>
#: en/advanced-use.xml:516
#, no-wrap
msgid "Module name : <emphasis>invert</emphasis>"
msgstr "Nom du module : <emphasis>invert</emphasis>"

# type: Content of: <chapter><sect1><sect2><sect3><title>
#: en/advanced-use.xml:520
#, no-wrap
