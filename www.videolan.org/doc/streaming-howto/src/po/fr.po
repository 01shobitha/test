msgstr "deinterlace"

# type: Content of: <chapter><sect1><sect2><sect3><para>
#: en/cmdline.xml:720
#, no-wrap
msgid ""
"This option allows to enable deinterlacing of interlaced video\n"
"streams before encoding."
msgstr "Cette option permet d'activer le dÃ©sentrelacement de vidÃ©os entrelacÃ©es avant d'encoder."

# type: Content of: <chapter><sect1><sect2><sect3><title>
#: en/cmdline.xml:725
#, no-wrap
msgid "croptop="
msgstr "croptop="

# type: Content of: <chapter><sect1><sect2><sect3><para>
#: en/cmdline.xml:727
#, no-wrap
