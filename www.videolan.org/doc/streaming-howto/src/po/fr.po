# type: Content of: <chapter><sect1><itemizedlist><listitem><para><filename>
#: en/dvd.xml:45
#, no-wrap
msgid "D:"
msgstr ""

# type: Content of: <chapter><sect1><itemizedlist><listitem><para>
#: en/dvd.xml:45
#, no-wrap
